                        "type": "string",
                        "description": "User's email",
                        "format": "email"
                    },
                    "password": {
                        "type": "string",
                        "description": "User's password",
                        "example": "qwerty123"
                    }
                }
            },
            "RegistrationResponse201": {
                "type": "object",
                "properties": {
                    "user": {
                        "type": "object",
                        "properties": {
                            "email": {
                                "type": "string",
                                "description": "User's email",
                                "format": "email"
                            }
                        },
                        "description": "User's info"
                    }
                },
                "example": {
                    "user": {
                        "email": "example@gmail.com"
                    }
                }
            },
            "RegistrationResponse401": {
